@model IEnumerable<TerminologyDemo.Models.ProjectUpload>

@{
    ViewData["Title"] = "Delete news";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <hr />
    
        

        
        
    <form asp-action="DeleteUrl">
       
     <dl class="row">
        <dt class = "col-sm-2">
         @Html.DropDownList(
                        "PId", 
                        new SelectList(
                        Model.Select(x => new { Value = x.PId, Text = x.NewsTitle}),
                         "Value",
                          "Text"
    )
) 
